<div class='center'>
  
    <h1>Listing Todo Lists</h1>

  <table class="table table-striped">
    <thead>
      <tr>
        <th>List name</th>
        <th>List due date</th>
        <th colspan="3"></th>
      </tr>
    </thead>

    <tbody >
      <% @todo_lists.each do |todo_list| %>
        <tr >
          <td ><%= todo_list.list_name %></td>
          <td><%= todo_list.list_due_date %></td>
          <td >
            <div class="btn-group" role="group" aria-label="..." >
              <%= link_to 'Show', todo_list, class: "btn btn-xs btn-success" %>
              <%= link_to 'Edit', edit_todo_list_path(todo_list), class: "btn btn-xs btn-warning" %>
              <%= link_to 'Destroy', todo_list, method: :delete, class: "btn btn-xs btn-danger"  %> </div> 
          </td>
        </tr>
      <% end %>
    </tbody>
  </table>
  <p/>

  <%= will_paginate @todo_lists %>
  <br>

  <%= link_to 'Create New Todo list', new_todo_list_path, class: "btn btn-xs btn-info"   %>
</div>